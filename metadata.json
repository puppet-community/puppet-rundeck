{
  "name":         "puppet-rundeck",
  "version":      "2.1.0",
  "author":       "voxpupuli",
  "license":      "Apache-2.0",
  "summary":      "Module for managing the installation and configuration of the rundeck orchestration tool",
  "source":       "https://github.com/voxpupuli/puppet-rundeck",
  "project_page": "https://github.com/voxpupuli/puppet-rundeck",
  "issues_url":   "https://github.com/voxpupuli/puppet-rundeck/issues",
  "tags": ["orchestration", "rundeck"],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": ["5.0","6.0","7.0"]
    },
    { "operatingsystem": "CentOS",
      "operatingsystemrelease": ["5.0","6.0", "7.0"]
    },
    { "operatingsystem": "Debian",
      "operatingsystemrelease": ["6.0","7.0"]
    },
    { "operatingsystem": "Ubuntu",
      "operatingsystemrelease": ["12.04","14.04"]
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 3.0.0 <5.0.0"
    },
    {
      "name": "puppetlabs/inifile",
      "version_requirement": ">= 1.0.3 <2.0.0"
    },
    {
      "name": "voxpupuli/archive",
      "version_requirement": ">= 0.3.0"
    }
  ]
}
