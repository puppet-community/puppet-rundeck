authentication {
<%- $rundeck::config::auth_config.each  |$_type| { -%>
  <%- if $_type in ['ldap', 'ldap_shared'] { -%>
    <%= epp('rundeck/_auth_ldap.epp') %>
  <%- } elsif ['active_directory', 'active_directory_shared'] { -%>
    <%= epp('rundeck/_auth_ad.epp') %>
  <%- } elsif $_type == 'pam' { -%>
    <%= epp('rundeck/_auth_pam.epp') %>
  <%- } elsif $_type == 'file' { -%>
    <%= epp('rundeck/_auth_file.epp') %>
  <%- } -%>
<%- } -%>
};
