authentication {
<%- $rundeck::config::auth_config.each  |$_type| { -%>
  <%- if 'ldap' in $type or 'ldap_shared' in $type { -%>
    <%= epp('rundeck/_auth_ldap.epp') %>
  <%- } elsif 'active_directory' in $type or 'active_directory_shared' in $type { -%>
    <%= epp('rundeck/_auth_ad.epp') %>
  <%- } elsif 'pam' in $type { -%>
    <%= epp('rundeck/_auth_pam.epp') %>
  <%- } elsif 'file' in $type { -%>
    <%= epp('rundeck/_auth_file.epp') %>
  <%- } -%>
<%- } -%>
};
